{
  "name": "kildekoden",
  "description": "Source code repository explorer. Lookup and explore open source projects.",
  "type": "project",
  "keywords" : ["open source"],
  "homepage" : "http://cakephp.org",
  "license" : "MIT",
  "require" : {
    "php" : ">=5.4",
    "composer/installers" : "~1.0",
    "cakephp/cakephp" : "2.6.*",
    "friendsofcake/crud" : "3.*",
    "josegonzalez/dotenv": "0.3.*",
    "ad7six/dsn": "~0.4",
    "ichikaway/cakephp-mongodb": "2.2.x-dev",
    "loadsys/cakephp_sitemap": "~1.0"
  },
  "require-dev": {
    "cakephp/debug_kit" : "2.2.*",
    "phpunit/phpunit": "3.7.32"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "heroku": {
      "framework": "cakephp2",
      "document-root": "webroot",
      "index-document": "index.php"
    }
  }
}
